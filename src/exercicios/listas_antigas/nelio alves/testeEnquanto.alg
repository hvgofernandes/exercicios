algoritmo "testeEnquanto"

var
   numero, soma: real
inicio
      escreval("Digite um número: ")
               leia(numero)
      Enquanto (numero <> 0) faca
               soma <- soma + numero
               escreval("Digite outro número: ")
               leia(numero)
      fimEnquanto
      
      escreval("A soma de todos os valores digitados é de: ", soma)
      



fimalgoritmo
