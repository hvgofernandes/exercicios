algoritmo "Calculador de idade"

var
   ano, ano1, idade: inteiro
inicio
      Escreval("Bem-vindo ao calculador de idade!")
      Escreval("Em qual anos nos estamos?")
      Leia(ano1)
      Escreval("Digite o ano em que você nasceu.")
      Leia(ano)
      idade <- ano1 - ano
      Escreva("Sua idade é igual a:", idade)
      
fimalgoritmo
