algoritmo "primeiro teste com vetores"

var
   valor: vetor[1..10] de inteiro
   contador: inteiro
inicio
      para contador <- 1 ate 10 faca
      Escreva("Digite o ", contador, "o. valor : ")
      Leia(valor[contador])
      fimPara
      
      para contador <- 1 ate 10 faca
           Escreva(valor[contador])
      fimPara
fimalgoritmo
