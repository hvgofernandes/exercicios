algoritmo "Conversor de dólares"

var
   reais: inteiro
   dolar: real
inicio
      Escreval("Seja bem-vindo ao conversor de dólares baseado no real!")
      Escreva("Quantos reais você tem? ")
      Leia(reais)
      dolar <- reais*0.16
      
      Escreva("Com", reais, " reais, você consegue comprar", dolar, " dólares.")
      
fimalgoritmo
